<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.javafx2.ViewController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <children>
      <ComboBox fx:id="comboBoxPerson" onAction="#setComboBoxPersonAction" prefWidth="150.0" promptText="Select a person" />
      <Button fx:id="btAll" mnemonicParsing="false" onAction="#obBtAllAction" text="All" />
   </children>
</VBox>
